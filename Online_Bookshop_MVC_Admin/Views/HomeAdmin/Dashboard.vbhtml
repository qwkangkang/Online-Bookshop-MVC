    // Show tooltip at specified coordinates
    function showTooltip(region, marker) {
        const rect = marker.getBoundingClientRect(); // Get marker's position relative to viewport
        const tooltipWidth = tooltip.offsetWidth;
        tooltip.textContent = region;
        //tooltip.style.left = (rect.left + rect.width / 2) + 'px';  // Adjust for marker's width
        //tooltip.style.top = (rect.top - 30) + 'px'; // Move above the marker
        // tooltip.style.top = 'px'; // Move above the marker
        const markerX = rect.left + rect.width / 2;
        const markerY = parseFloat(marker.getAttribute('cy'));

        tooltip.innerHTML = region;
        console.log("left "+(markerX - tooltipWidth / 2));
        tooltip.style.left =  (markerX - tooltipWidth / 2) + 'px'; // Center the tooltip horizontally
        tooltip.style.top =(parseFloat(marker.getAttribute('cy')) + 30)+ 'px';
        tooltip.style.display = 'block';
        marker.setAttribute('r', 5); // Radius of the marker
    };

    // Hide tooltip
    function hideTooltip(marker) {
        tooltip.style.display = 'none';
        marker.setAttribute('r', 3); // Radius of the marker
    };



