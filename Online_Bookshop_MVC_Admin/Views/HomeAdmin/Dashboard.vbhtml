@ModelType ModelLibrary.Dashboard
@Code
    ViewData("Title") = "Dashboard"
    Layout = "~/Views/Shared/_LayoutPage.vbhtml"

    Dim monthListJson = ViewBag.MonthListJson
    Dim monthRevenueFictionJson = ViewBag.MonthRevenueFictionJson
    Dim monthRevenueNonFictionJson = ViewBag.MonthRevenueNonFictionJson
    Dim agePListJson = ViewBag.AgePListJson
End Code

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/cdbootstrap/css/bootstrap.min.css" />
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/cdbootstrap/css/cdb.min.css" />
<script src="https://cdn.jsdelivr.net/npm/cdbootstrap/js/cdb.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/cdbootstrap/js/bootstrap.min.js"></script>
<script src="https://kit.fontawesome.com/9d1d9a82d2.js" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.js"></script>
@*<script src="https://unpkg.com/tooltip.js@1.4.10/dist/umd/tooltip.min.js"></script>*@

<style>
    .cell-container {
        border-radius: 10px;
        box-shadow: 0 20px 30px 0 rgba(138, 155, 165, 0.15);
        background: white;
        margin: 5px;
        padding: 10px;
    }

    .fa-long-arrow-up{
        color:#4cff00;
    }

    .fa-long-arrow-down{       
        color:#ff6a00;
    }

    #chartjs-doughnut{
        height:255px;
        width:200px;
    }

    #myChart{
        max-height:500px;
        max-width:500px;
        margin:0 auto
    }

    #vector-map {
    height: 420px;
    width: 100%;
    display: block;
}

    #map {
            height: 60vh;
        }

        .tooltip1 {
            position: absolute;
            background-color: rgba(0, 0, 0, 0.8);
            color: white;
            padding: 5px;
            border-radius: 5px;
            display: none;
            z-index:10;
            max-width:200px;
            text-align:left;
        }

        .text-center{
            margin:0;
            padding:0;
            position:relative;
        }
</style>

<div class="tables-container">
    <div>
        <table style="width:90%; margin:0 auto;">
            <tr>
                <td width="33.33%" height="100" colspan="2">
                    <div class="cell-container">

                        <figure class="text-center">
                            <h3>
                                Total Book
                            </h3>
                            <h2 class="display-3">
                                @Model.totalBook
                            </h2>
                            <h4>
                                <i id="bookPIcon" class="fa"></i> @String.Format("{0:0.00}%", Math.Abs(Model.bookP))
                            </h4>
                            <small>
                                vs previous 1 month
                            </small>
                        </figure>
                    </div>
                </td>
                <td width="33.33%" colspan="2">
                    <div class="cell-container">

                        <figure class="text-center">
                            <h3>
                                Total Customer
                            </h3>
                            <h2 class="display-3">
                                @Model.totalCustomer
                            </h2>
                            <h4>
                                <i id="customerPIcon" class="fa"></i> @String.Format("{0:0.00}%", Math.Abs(Model.customerP))
                            </h4>
                            <small>
                                vs previous 1 month
                            </small>
                        </figure>
                    </div>
                </td>
                <td width="33.33%" colspan="2">
                    <div class="cell-container">

                        <figure class="text-center">
                            <h3>
                                Orders per Month
                            </h3>
                            <h2 class="display-3">
                                @Model.totalOrder
                            </h2>
                            <h4>
                                <i id="orderPIcon" class="fa"></i> @String.Format("{0:0.00}%", Math.Abs(Model.orderP))
                            </h4>
                            <small>
                                vs previous 1 month
                            </small>
                        </figure>
                    </div>
                </td>
            </tr>
            <tr>

                <td colspan="3" height="180">
                    <div class="cell-container">
                        <figure class="text-center">
                            <h3>
                                Customer Age Group
                            </h3>
                        </figure>
                        @*<canvas id="line-chart"></canvas>*@

                        <div class="wrapper">
                            <canvas id="chartjs-doughnut"></canvas>
                        </div>


                        @*<div class="card chart-container">
                    <canvas id="chart"></canvas>
                </div>*@
                    </div>
                </td>
          
                <td colspan="3" height="180">
                    <div class="cell-container" style="height:310px;">
                        <figure class="text-center">
                            <h3>
                                Revenue in 3 Months
                            </h3>
                            <div class="wrapper" style="margin:0 auto">
                                <canvas id="myChart"></canvas>
                            </div>
                        </figure>
                        @*<canvas id="line-chart"></canvas>*@




                        @*<div class="card chart-container">
                    <canvas id="chart"></canvas>
                </div>*@
                    </div>
                </td>
            </tr>
           
            @* test vector map *@
            <tr>

                
                <td colspan="6" height="200">
                    <div class="cell-container">
                        <figure class="text-center">
                            <h3>
                                Bookshop Branch Location
                            </h3>
                            @*<div class="wrapper1" id="mapWrapper" style="margin:0 auto">*@
                                @*<div class="dx-viewport demo-container">
                                    <div id="vector-map"></div>
                                    <div id="button"></div>
                                </div>*@
                            <div id="map"></div>
                            <div class="tooltip1" id="tooltip" data-bs-toggle="tooltip" data-bs-html="true"></div>

                                
                            @*</div>*@
                        </figure>
                        @*<canvas id="line-chart"></canvas>*@




                        @*<div class="card chart-container">
                        <canvas id="chart"></canvas>
                    </div>*@
                    </div>
                </td>
            </tr>
        </table>
    </div>
</div>

<script>
    var tooltipTriggerList = [].slice.call(document.querySelectorAll('.tooltip1[data-bs-toggle="tooltip"]'));
    var tooltipList = tooltipTriggerList.map(function (tooltipTriggerEl) {
        return new bootstrap.Tooltip(tooltipTriggerEl);
    });
    //summary data
    window.onload = function() {
        var bookP = @Model.bookP;
        var bookPIcon = document.getElementById("bookPIcon");

        if(bookP <0){
            bookPIcon.classList.add("fa-long-arrow-down");
            console.log("negative");
        }else{
            bookPIcon.classList.add("fa-long-arrow-up");
            console.log("positive");
        }

        var customerP = @Model.customerP;
        var customerPIcon = document.getElementById("customerPIcon");

        if(customerP <0){
            customerPIcon.classList.add("fa-long-arrow-down");
            console.log("negative");
        }else{
            customerPIcon.classList.add("fa-long-arrow-up");
            console.log("positive");
        }

        var orderP = @Model.orderP;
        var orderPIcon = document.getElementById("orderPIcon");

        if(orderP <0){
            orderPIcon.classList.add("fa-long-arrow-down");
            console.log("negative");
        }else{
            orderPIcon.classList.add("fa-long-arrow-up");
            console.log("positive");
        }
    }

    //pie chart
    var agePList = JSON.parse('@Html.Raw(agePListJson)');

    new Chart(document.getElementById("chartjs-doughnut"), {
        type: "doughnut",
        data: {
            labels: ["Below 18", "19 to 24", "25 to 34", "35 to 44", "45 to 54", "55 and above"],
            datasets: [{
                data: agePList,
                backgroundColor: [
                  "#FF5733",
                  "#FFBD33",
                  "#DBFF33",
                  "#75FF33",
                  "#33FF57",
                  "#33FFBD"
                ],
                borderColor: "transparent"
            }]
        },
        options: {
            maintainAspectRatio: false,
            cutoutPercentage: 55,
            legend: {
                position: "bottom",
                align: "center" 
            }
        }
    });




    // line Chart
    var ctx2 = document.getElementById('myChart').getContext("2d");
    var monthList = JSON.parse('@Html.Raw(monthListJson)');
    var monthRevenueFiction = JSON.parse('@Html.Raw(monthRevenueFictionJson)');
    var monthRevenueNonFiction = JSON.parse('@Html.Raw(monthRevenueNonFictionJson)');
    //console.log("month list "+monthListJson);
    //var monthList = ConvertArrayToJson(@Model.monthList);


    var myChart2 = new Chart(ctx2, {
        type: 'line',
        data: {
            labels: monthList,
            datasets: [{
                label: "Fiction",
                borderColor: "#80b6f4",
                pointBorderColor: "#80b6f4",
                pointBackgroundColor: "#80b6f4",
                pointHoverBackgroundColor: "#80b6f4",
                pointHoverBorderColor: "#80b6f4",
                pointBorderWidth: 10,
                pointHoverRadius: 10,
                pointHoverBorderWidth: 1,
                pointRadius: 3,
                fill: false,
                borderWidth: 4,
                data: monthRevenueFiction
            },
		{
		    label: "Non-Fiction",
		    borderColor: "#d91882",
		    pointBorderColor: "#d91882",
		    pointBackgroundColor: "#d91882",
		    pointHoverBackgroundColor: "#d91882",
		    pointHoverBorderColor: "#d91882",
		    pointBorderWidth: 10,
		    pointHoverRadius: 10,
		    pointHoverBorderWidth: 1,
		    pointRadius: 3,
		    fill: false,
		    borderWidth: 4,
		    data: monthRevenueNonFiction
		}]
        },
